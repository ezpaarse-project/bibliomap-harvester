{
  "name": "ezpaarse2log.io-harvester",
  "version": "1.3.0",
  "description": "Script monitoring ezproxy log files and sending it to ezpaarse2log.io before being displayed into bibliomap",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel demo-logs-injector log.io-harvester",
    "log.io-harvester": "HOME=$(pwd) ./node_modules/.bin/log.io-harvester",
    "demo-logs-injector": "./demo-logs-injector.sh",
    "insertversion": "sed -i \"s#\\(image: ezpaarseproject/ezpaarse2log.io-harvester:\\)\\([\\.a-z0-9]\\+\\)#\\1${npm_package_version}#g\" docker-compose.yml",
    "version": "npm run insertversion && git add docker-compose.yml",
    "postversion": "git push && git push --tags"
  },
  "contributors": [
    {
      "name": "St√©phane Gully",
      "url": "https://github.com/kerphi"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "log.io": "^0.3.4",
    "npm-run-all": "^3.1.2"
  },
  "devDependencies": {}
}
